    "cli",
    "repl",
    "chromium"
  ],
  "author": "mj1618",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mj1618/playwright-cli.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}

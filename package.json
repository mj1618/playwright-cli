{
  "name": "@mj1618/playwright-cli",
  "version": "1.6.0",
  "description": "A CLI for running Playwright commands against a persistent browser session",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "playwright-cli": "./bin/playwright-cli.js"
  },
  "files": [
    "bin/",
    "dist/",
    "LICENSE"
  ],
  "scripts": {
